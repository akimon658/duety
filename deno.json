{
  "lock": false,
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check **/*.ts && deno check **/*.tsx",
    "dev": "deno run -A --watch=static/,routes/ dev.ts",
    "build": "deno run -A dev.ts build",
    "start": "deno run -A main.ts",
    "preview": "deno run -A main.ts",
    "update": "deno run -A -r jsr:@fresh/update .",
    "db:generate": "deno run -A --node-modules-dir npm:drizzle-kit generate",
    "db:migrate": "deno run -A --node-modules-dir npm:drizzle-kit migrate",
    "db:push": "deno run -A --node-modules-dir npm:drizzle-kit push",
    "db:studio": "deno run -A --node-modules-dir npm:drizzle-kit studio"
  },
  "compilerOptions": {
    "lib": ["dom", "dom.asynciterable", "dom.iterable", "deno.ns"],
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "imports": {
    "@preact/signals": "https://esm.sh/*@preact/signals@1.3.0",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.8.0",
    "@std/http": "jsr:@std/http@^1.0.0",
    "@std/dotenv/": "jsr:@std/dotenv@^0.225.0/",
    "$fresh/": "https://deno.land/x/fresh@1.7.3/",
    "preact": "https://esm.sh/preact@10.24.1",
    "preact/": "https://esm.sh/preact@10.24.1/",
    "drizzle-orm": "npm:drizzle-orm@^0.38.0",
    "drizzle-orm/": "npm:drizzle-orm@^0.38.0/",
    "drizzle-kit": "npm:drizzle-kit@^0.30.0",
    "mysql2": "npm:mysql2@^3.11.0",
    "mysql2/promise": "npm:mysql2@^3.11.0/promise",
    "ical.js": "npm:ical.js@^2.1.0",
    "@/": "./",
    "tailwindcss": "npm:tailwindcss@3.4.1",
    "tailwindcss/": "npm:tailwindcss@3.4.1/",
    "tailwindcss/plugin": "npm:tailwindcss@3.4.1/plugin.js"
  },
  "exclude": ["**/_fresh/*", "node_modules"]
}
